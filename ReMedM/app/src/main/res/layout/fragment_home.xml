<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_principal">

    <TextView
        android:id="@+id/Perfil"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/bg_blanco_redondo"
        app:layout_constraintEnd_toEndOf="@id/Perfil"
        app:layout_constraintStart_toStartOf="@id/Perfil"
        app:layout_constraintTop_toTopOf="@id/Perfil"/>

    <ImageView
        android:id="@+id/IP"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/perfil"
        app:layout_constraintEnd_toEndOf="@id/Perfil"
        app:layout_constraintHorizontal_bias="0.210"
        app:layout_constraintStart_toStartOf="@id/Perfil"
        app:layout_constraintTop_toTopOf="@id/Perfil"/>
    <EditText
        android:id="@+id/nombre"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="8dp"
        android:background="@color/cardview_light_background"
        android:text="Nombre"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@id/Perfil"
        app:layout_constraintHorizontal_bias="0.699"
        app:layout_constraintStart_toStartOf="@id/Perfil"
        app:layout_constraintTop_toTopOf="@id/Perfil"/>
    <ImageView
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="110dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/bg_blanco_redondo"
        app:layout_constraintEnd_toEndOf="@id/Perfil"
        app:layout_constraintStart_toStartOf="@id/Perfil"
        app:layout_constraintTop_toTopOf="@id/Perfil"/>

    <ImageView
        android:id="@+id/IP2"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="120dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/perfil"
        app:layout_constraintEnd_toEndOf="@id/Perfil"
        app:layout_constraintHorizontal_bias="0.210"
        app:layout_constraintStart_toStartOf="@id/Perfil"
        app:layout_constraintTop_toTopOf="@id/Perfil"/>
    <EditText
        android:id="@+id/apaterno"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="110dp"
        android:layout_marginEnd="8dp"
        android:background="@color/cardview_light_background"
        android:text="Apellido Paterno"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@id/Perfil"
        app:layout_constraintHorizontal_bias="0.699"
        app:layout_constraintStart_toStartOf="@id/Perfil"
        app:layout_constraintTop_toTopOf="@id/Perfil"/>
    <ImageView
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="170dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/bg_blanco_redondo"
        app:layout_constraintEnd_toEndOf="@id/Perfil"
        app:layout_constraintStart_toStartOf="@id/Perfil"
        app:layout_constraintTop_toTopOf="@id/Perfil"/>

    <ImageView
        android:id="@+id/IP3"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="180dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/perfil"
        app:layout_constraintEnd_toEndOf="@id/Perfil"
        app:layout_constraintHorizontal_bias="0.210"
        app:layout_constraintStart_toStartOf="@id/Perfil"
        app:layout_constraintTop_toTopOf="@id/Perfil"/>
    <EditText
        android:id="@+id/amaterno"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="170dp"
        android:layout_marginEnd="8dp"
        android:background="@color/cardview_light_background"
        android:text="Apellido Materno"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@id/Perfil"
        app:layout_constraintHorizontal_bias="0.699"
        app:layout_constraintStart_toStartOf="@id/Perfil"
        app:layout_constraintTop_toTopOf="@id/Perfil"/>
    <ImageView
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="230dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/bg_blanco_redondo"
        app:layout_constraintEnd_toEndOf="@id/Perfil"
        app:layout_constraintStart_toStartOf="@id/Perfil"
        app:layout_constraintTop_toTopOf="@id/Perfil"/>

    <ImageView
        android:id="@+id/IP4"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="240dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/calendar"
        app:layout_constraintEnd_toEndOf="@id/Perfil"
        app:layout_constraintHorizontal_bias="0.210"
        app:layout_constraintStart_toStartOf="@id/Perfil"
        app:layout_constraintTop_toTopOf="@id/Perfil"/>
    <EditText
        android:id="@+id/Fecha_nac"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="230dp"
        android:layout_marginEnd="8dp"
        android:background="@color/cardview_light_background"
        android:text="13/10/1995"
        android:inputType="date"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@id/Perfil"
        app:layout_constraintHorizontal_bias="0.699"
        app:layout_constraintStart_toStartOf="@id/Perfil"
        app:layout_constraintTop_toTopOf="@id/Perfil"/>
    <ImageView
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="290dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/bg_blanco_redondo"
        app:layout_constraintEnd_toEndOf="@id/Perfil"
        app:layout_constraintStart_toStartOf="@id/Perfil"
        app:layout_constraintTop_toTopOf="@id/Perfil"/>

    <ImageView
        android:id="@+id/IP5"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="300dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/ubi"
        app:layout_constraintEnd_toEndOf="@id/Perfil"
        app:layout_constraintHorizontal_bias="0.210"
        app:layout_constraintStart_toStartOf="@id/Perfil"
        app:layout_constraintTop_toTopOf="@id/Perfil"/>
    <EditText
        android:id="@+id/domicilio"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="290dp"
        android:layout_marginEnd="8dp"
        android:background="@color/cardview_light_background"
        android:text="Domicilio"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@id/Perfil"
        app:layout_constraintHorizontal_bias="0.699"
        app:layout_constraintStart_toStartOf="@id/Perfil"
        app:layout_constraintTop_toTopOf="@id/Perfil"/>
    <ImageView
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="350dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/bg_blanco_redondo"
        app:layout_constraintEnd_toEndOf="@id/Perfil"
        app:layout_constraintStart_toStartOf="@id/Perfil"
        app:layout_constraintTop_toTopOf="@id/Perfil"/>

    <ImageView
        android:id="@+id/IP6"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="360dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/tel"
        app:layout_constraintEnd_toEndOf="@id/Perfil"
        app:layout_constraintHorizontal_bias="0.210"
        app:layout_constraintStart_toStartOf="@id/Perfil"
        app:layout_constraintTop_toTopOf="@id/Perfil"/>
    <EditText
        android:id="@+id/telefono"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="350dp"
        android:layout_marginEnd="8dp"
        android:background="@color/cardview_light_background"
        android:text="Telefono"
        android:inputType="phone"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@id/Perfil"
        app:layout_constraintHorizontal_bias="0.699"
        app:layout_constraintStart_toStartOf="@id/Perfil"
        app:layout_constraintTop_toTopOf="@id/Perfil"/>
    <ImageView
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="410dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/bg_blanco_redondo"
        app:layout_constraintEnd_toEndOf="@id/Perfil"
        app:layout_constraintStart_toStartOf="@id/Perfil"
        app:layout_constraintTop_toTopOf="@id/Perfil"/>

    <ImageView
        android:id="@+id/IP7"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="420dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/correo"
        app:layout_constraintEnd_toEndOf="@id/Perfil"
        app:layout_constraintHorizontal_bias="0.210"
        app:layout_constraintStart_toStartOf="@id/Perfil"
        app:layout_constraintTop_toTopOf="@id/Perfil"/>
    <EditText
        android:id="@+id/correo"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="410dp"
        android:layout_marginEnd="8dp"
        android:background="@color/cardview_light_background"
        android:text="Correo"
        android:inputType="textEmailAddress"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@id/Perfil"
        app:layout_constraintHorizontal_bias="0.699"
        app:layout_constraintStart_toStartOf="@id/Perfil"
        app:layout_constraintTop_toTopOf="@id/Perfil"/>
    <ImageView
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="470dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/bg_blanco_redondo"
        app:layout_constraintEnd_toEndOf="@id/Perfil"
        app:layout_constraintStart_toStartOf="@id/Perfil"
        app:layout_constraintTop_toTopOf="@id/Perfil"/>

    <ImageView
        android:id="@+id/IP8"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="480dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/contra"
        app:layout_constraintEnd_toEndOf="@id/Perfil"
        app:layout_constraintHorizontal_bias="0.210"
        app:layout_constraintStart_toStartOf="@id/Perfil"
        app:layout_constraintTop_toTopOf="@id/Perfil"/>
    <EditText
        android:id="@+id/Contrasenia"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="470dp"
        android:layout_marginEnd="8dp"
        android:background="@color/cardview_light_background"
        android:text="Contraseña"
        android:inputType="textPassword"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@id/Perfil"
        app:layout_constraintHorizontal_bias="0.699"
        app:layout_constraintStart_toStartOf="@id/Perfil"
        app:layout_constraintTop_toTopOf="@id/Perfil"/>
    <Button
        android:id="@+id/BtnModificar"
        android:layout_width="120dp"
        android:layout_height="50dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="560dp"
        android:layout_marginEnd="8dp"
        android:text="Modificar"
        android:textColor="@color/cardview_light_background"
        android:background="@drawable/bg_verde_redondo"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@id/Perfil"
        app:layout_constraintHorizontal_bias="0.210"
        app:layout_constraintStart_toStartOf="@id/Perfil"
        app:layout_constraintTop_toTopOf="@id/Perfil"/>
    <Button
        android:id="@+id/BtnCancelar"
        android:layout_width="120dp"
        android:layout_height="50dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="560dp"
        android:layout_marginEnd="8dp"
        android:text="Cancelar"
        android:textColor="@color/cardview_light_background"
        android:background="@drawable/bg_verde_redondo"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@id/Perfil"
        app:layout_constraintHorizontal_bias="0.779"
        app:layout_constraintStart_toStartOf="@id/Perfil"
        app:layout_constraintTop_toTopOf="@id/Perfil"/>





</androidx.constraintlayout.widget.ConstraintLayout>